{
  "uid": "kart-api",
  "title": "Kart API",
  "editable": true,
  "timezone": "browser",
  "refresh": "10s",
  "time": { "from": "now-15m", "to": "now" },
  "templating": { "list": [] },
  "panels": [
    {
      "type": "row",
      "title": "HTTP Overview",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Request Rate by Route",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": { "drawStyle": "line", "fillOpacity": 10 }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "sum by (http_route) (rate(http_server_request_duration_seconds_count[5m]))",
          "legendFormat": "{{ http_route }}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "p95 Latency by Route",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": { "drawStyle": "line", "fillOpacity": 10 }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, http_route) (rate(http_server_request_duration_seconds_bucket[5m])))",
          "legendFormat": "{{ http_route }}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Error Rate",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": { "drawStyle": "line", "fillOpacity": 10 }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "sum by (http_route) (rate(http_server_request_duration_seconds_count{http_response_status_code=~\"[45]..\"}[5m]))",
          "legendFormat": "{{ http_route }}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "row",
      "title": "ogen Operations",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 9 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Request Count by Operation",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 10 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": { "drawStyle": "line", "fillOpacity": 10 }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "sum by (ogen_operation) (rate(ogen_server_request_count_total[5m]))",
          "legendFormat": "{{ ogen_operation }}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Operation Duration p95",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 10 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": { "drawStyle": "line", "fillOpacity": 10 }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, ogen_operation) (rate(ogen_server_duration_milliseconds_bucket[5m])))",
          "legendFormat": "{{ ogen_operation }}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "row",
      "title": "Go Runtime",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 18 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Goroutine Count",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 19 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": { "drawStyle": "line", "fillOpacity": 10 }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "go_goroutine_count",
          "legendFormat": "goroutines",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Memory Usage",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 19 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": { "drawStyle": "line", "fillOpacity": 10 }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "go_memory_used_bytes",
          "legendFormat": "used",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Memory Allocation Rate",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 19 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
          "custom": { "drawStyle": "line", "fillOpacity": 10 }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "rate(go_memory_allocated_bytes_total[5m])",
          "legendFormat": "alloc rate",
          "refId": "A"
        }
      ]
    }
  ],
  "schemaVersion": 39
}
